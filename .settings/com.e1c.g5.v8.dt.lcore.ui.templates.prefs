2 \u043D\u0435\u0439 \u0443\u043A\u0430\u0437\u044B\u0432\u0430\u0435\u0442\u0441\u044F e-mail \u0434\u043B\u044F \u043E\u0442\u043F\u0440\u0430\u0432\u043A\u0438. \\r\\n//\\t\\t\u0412 \u0441\u043B\u0443\u0447\u0430\u0435, \u0435\u0441\u043B\u0438 \u0443\u043A\u0430\u0437\u0430\u043D \u044D\u043B\u0435\u043C\u0435\u043D\u0442 \u0441\u043F\u0440\u0430\u0432\u043E\u0447\u043D\u0438\u043A\u0430 \u041F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u0438, \u0438\u043C\u044F \u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u044F \u0431\u0435\u0440\u0435\u0442\u0441\u044F \u0438\u0437 \u043D\u0430\u0438\u043C\u0435\u043D\u043E\u0432\u0430\u043D\u0438\u044F, e-mail - \u0438\u0437 \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0438 is\u0410\u0434\u0440\u0435\u0441\u042D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u043E\u0439\u041F\u043E\u0447\u0442\u044B \u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u044F.\\r\\n//\\t\\t\u0412 \u0441\u043B\u0443\u0447\u0430\u0435 \u0443\u043A\u0430\u0437\u0430\u043D\u0438\u044F \u0442\u0430\u0431. \u0437\u043D\u0430\u0447., \u043C\u043E\u0436\u043D\u043E \u0443\u043A\u0430\u0437\u0430\u0442\u044C \u043D\u0435\u0441\u043A\u043E\u043B\u044C\u043A\u043E \u0430\u0434\u0440\u0435\u0441\u043E\u0432 \u0434\u043B\u044F \u0434\u043E\u0441\u0442\u0430\u0432\u043A\u0438. \u0422\u0417 \u0434\u043E\u043B\u0436\u043D\u0430 \u0438\u043C\u0435\u0442\u044C \u043A\u043E\u043B\u043E\u043D\u043A\u0438 \\r\\n//\\t\\t\\t\u0410\u0434\u0440\u0435\u0441\:\u0421\u0442\u0440\u043E\u043A\u0430(\u041F50) - e-mail \u0434\u043B\u044F \u043E\u0442\u043F\u0440\u0430\u0432\u043A\u0438\\r\\n//\\t\\t\\t\u041F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u044C\:\u0421\u043F\u0440\u0430\u0432\u043E\u0447\u043D\u0438\u043A\u0421\u0441\u044B\u043B\u043A\u0430.\u041F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u0438 - \u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u044C \u0434\u043B\u044F \u043E\u0442\u043F\u0440\u0430\u0432\u043A\u0438. \u0418\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u0435\u0442\u0441\u044F \u0434\u043B\u044F \u0443\u043A\u0430\u0437\u0430\u043D\u0438\u044F \u0438\u043C\u0435\u043D\u0438 \u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u044F, \u0430 \u0435\u0441\u043B\u0438 e-mail \u0438\u043C\u0435\u0435\u0442 \u0442\u0438\u043F \u043E\u0442\u043B\u0438\u0447\u043D\u044B\u0439 \u043E\u0442 \u0421\u0442\u0440\u043E\u043A\u0430, \u0442\u043E e-mail \u0431\u0435\u0440\u0435\u0442\u0441\u044F \u0438\u0437 \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0438 is\u0410\u0434\u0440\u0435\u0441\u042D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u043E\u0439\u041F\u043E\u0447\u0442\u044B \u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u0435\u043B\u044F\\r\\n//\\t\\t\u0415\u0441\u043B\u0438 \u0422\u0417 \u0441\u043E\u0434\u0435\u0440\u0436\u0438\u0442 \u043D\u0435\u043A\u043E\u0440\u0440\u0435\u043A\u0442\u043D\u044B\u0435 \u0434\u0430\u043D\u043D\u044B\u0435, \u0441\u0442\u0440\u043E\u043A\u0430 \u043F\u0440\u043E\u043F\u0443\u0441\u043A\u0430\u0435\u0442\u0441\u044F (\u043E\u0442\u043F\u0440\u0430\u0432\u043A\u0430 \u0434\u0430\u043D\u043D\u043E\u043C\u0443 \u043F\u043E\u043B\u0443\u0447\u0430\u0442\u0435\u043B\u044E \u043D\u0435 \u043F\u0440\u043E\u0438\u0437\u0432\u043E\u0434\u0438\u0442\u0441\u044F)\\r\\n//\\r\\n//\\t\u0422\u0435\u043C\u0430\:\u0421\u0442\u0440\u043E\u043A\u0430. \u0422\u0435\u043C\u0430 \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u044F\\r\\n//\\t\u0422\u0435\u043A\u0441\u0442\:\u0421\u0442\u0440\u043E\u043A\u0430. \u0422\u0435\u043A\u0441\u0442 \u043E\u0442\u043F\u0440\u0430\u0432\u043B\u044F\u0435\u043C\u043E\u0433\u043E \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u044F.\\r\\n//\u0412\u041E\u0417\u0412\u0420\u0410\u0429\u0410\u0415\u0422\\r\\n//  \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E, \u0415\u0441\u043B\u0438 \u043D\u0435 \u043D\u0430\u0441\u0442\u0440\u043E\u0435\u043D \u0430\u0434\u0440\u0435\u0441 \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u0442\u0435\u043B\u044F.\\r\\n//\\t\u0421\u043F\u0440\u0430\u0432\u043E\u0447\u043D\u0438\u043A\u0438.\u0443\u0443\u0420\u0430\u0441\u0441\u044B\u043B\u043A\u0430\u0421\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439\u041F\u043E\u042D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u043E\u0439\u041F\u043E\u0447\u0442\u0435.\u041F\u0443\u0441\u0442\u0430\u044F\u0421\u0441\u044B\u043B\u043A\u0430(), \u0435\u0441\u043B\u0438 \u0443\u043A\u0430\u0437\u0430\u043D \u043D\u0435\u043A\u043E\u0440\u0440\u0435\u043A\u0442\u043D\u044B\u0439 \u0430\u0434\u0440\u0435\u0441 \u043F\u043E\u043B\u0443\u0447\u0430\u0442\u0435\u043B\u044F\\r\\n//  \u041B\u043E\u0436\u044C, \u0435\u0441\u043B\u0438 \u043D\u0435 \u0443\u0434\u0430\u043B\u043E\u0441\u044C \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435 \u0434\u043B\u044F \u043E\u0442\u043F\u0440\u0430\u0432\u043A\u0438 (\u0437\u0430\u043F\u0438\u0441\u0430\u0442\u044C \u044D\u043B\u0435\u043C\u0435\u043D\u0442 \u0441\u043F\u0440\u0430\u0432\u043E\u0447\u043D\u0438\u043A\u0430)\\r\\n//  \u0418\u0441\u0442\u0438\u043D\u0430, \u0435\u0441\u043B\u0438 \u0443\u0434\u0430\u043B\u043E\u0441\u044C \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435 \u0434\u043B\u044F \u043E\u0442\u043F\u0440\u0430\u0432\u043A\u0438 (\u0437\u0430\u043F\u0438\u0441\u0430\u0442\u044C \u044D\u043B\u0435\u043C\u0435\u043D\u0442 \u0441\u043F\u0440\u0430\u0432\u043E\u0447\u043D\u0438\u043A\u0430)\\r\\n\u0424\u0443\u043D\u043A\u0446\u0438\u044F \u0417\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043E\u0432\u0430\u0442\u044C\u0421\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435\u0414\u043B\u044F\u041E\u0442\u043F\u0440\u0430\u0432\u043A\u0438\u041F\u043E\u041F\u043E\u0447\u0442\u0435(\u0410\u0434\u0440\u0435\u0441\u0430\u0442\u044B, \u0422\u0435\u043C\u0430, \u0422\u0435\u043A\u0441\u0442) \u042D\u043A\u0441\u043F\u043E\u0440\u0442\\r\\n\\t\u041E\u0442\u043F\u0440\u0430\u0432\u0438\u0442\u0435\u043B\u044C \\u003d \u041A\u043E\u043D\u0441\u0442\u0430\u043D\u0442\u044B.\u0443\u0443\u0410\u0434\u0440\u0435\u0441\u041E\u0442\u043F\u0440\u0430\u0432\u0438\u0442\u0435\u043B\u044F\u0421\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439\u041E\u041D\u0435\u0442\u0440\u0430\u043D\u0441\u043B\u0438\u0440\u0443\u0435\u043C\u044B\u0445\u041F\u0440\u043E\u0432\u043E\u0434\u043A\u0430\u0445.\u041F\u043E\u043B\u0443\u0447\u0438\u0442\u044C(); \\r\\n\\t\u0415\u0441\u043B\u0438 \u041D\u0415 \u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435\u0417\u0430\u043F\u043E\u043B\u043D\u0435\u043D\u043E(\u041E\u0442\u043F\u0440\u0430\u0432\u0438\u0442\u0435\u043B\u044C) \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\u0412\u043E\u0437\u0432\u0440\u0430\u0442 \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E;\\r\\n\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438;\\r\\n\\t\\r\\n\\t\u041F\u0443\u0441\u0442\u0430\u044F\u0421\u0441\u044B\u043B\u043A\u0430 \\u003d \u0421\u043F\u0440\u0430\u0432\u043E\u0447\u043D\u0438\u043A\u0438.\u0443\u0443\u0420\u0430\u0441\u0441\u044B\u043B\u043A\u0430\u0421\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439\u041F\u043E\u042D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u043E\u0439\u041F\u043E\u0447\u0442\u0435.\u041F\u0443\u0441\u0442\u0430\u044F\u0421\u0441\u044B\u043B\u043A\u0430();\\r\\n\\t\u0422\u0438\u043F\u0410\u0434\u0440\u0435\u0441\u0430\u0442\u0430 \\u003d \u0422\u0438\u043F\u0417\u043D\u0447(\u0410\u0434\u0440\u0435\u0441\u0430\u0442\u044B);\\r\\n\\t\u0422\u0438\u043F\u0421\u0442\u0440\u043E\u043A\u0430 \\u003d \u0422\u0438\u043F(\\"\u0421\u0442\u0440\u043E\u043A\u0430\\");\\r\\n\\t\u0422\u0438\u043F\u0421\u043F\u0440\u041F\u043E\u043B\u044C\u0437 \\u003d \u0422\u