E\u0432\u0435\u0440\u044F\u0435\u043C \\u003d \u0418\u0441\u0442\u0438\u043D\u0430 \u0438 \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C \\u003c\\u003e \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\\t\u0412\u043E\u0437\u0432\u0440\u0430\u0442 \u0418\u0441\u0442\u0438\u043D\u0430; \\r\\n\\t\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438; \\r\\n\\t\\t\\r\\n\\t\\t\u0415\u0441\u043B\u0438 \u0422\u0438\u043F\u0417\u043D\u0447(\u043A\u0437.\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435) \\u003d \u0422\u0438\u043F(\\"\u0421\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0435\\") \u0422\u043E\u0433\u0434\u0430 // \u0422\u0427\\r\\n\\t\\t\\t\u0414\u043B\u044F \u043A\u0430\u0436\u0434\u043E\u0433\u043E \u0418\u043C\u044F\u041A\u043E\u043B\u043E\u043D\u043A\u0438 \u0418\u0437 \u043A\u0437.\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435 \u0426\u0438\u043A\u043B\\r\\n\\t\\t\\t\\t\u0415\u0441\u043B\u0438 \u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C \\u003d \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E \u0438 \u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0435\u041A\u043E\u043B\u043E\u043D\u043A\u0438.\u0421\u0432\u043E\u0439\u0441\u0442\u0432\u043E(\u0418\u043C\u044F\u041A\u043E\u043B\u043E\u043D\u043A\u0438) \\u003d \u041B\u043E\u0436\u044C \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\\t\\t\\t\u041F\u0440\u043E\u0434\u043E\u043B\u0436\u0438\u0442\u044C;\\r\\n\\t\\t\\t\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438;\\r\\n\\t\\t\\t\\t\u0415\u0441\u043B\u0438 \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C \\u003d \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E \u0438 \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C\u044B\u0435\u041A\u043E\u043B\u043E\u043D\u043A\u0438.\u0421\u0432\u043E\u0439\u0441\u0442\u0432\u043E(\u0418\u043C\u044F\u041A\u043E\u043B\u043E\u043D\u043A\u0438) \\u003d \u0418\u0441\u0442\u0438\u043D\u0430 \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\\t\\t\\t\u041F\u0440\u043E\u0434\u043E\u043B\u0436\u0438\u0442\u044C;\\r\\n\\t\\t\\t\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438;\\r\\n\\t\\t\\t\\t\u0412\u043E\u0437\u0432\u0440\u0430\u0442 \u0418\u0441\u0442\u0438\u043D\u0430; \\r\\n\\t\\t\\t\u041A\u043E\u043D\u0435\u0446\u0426\u0438\u043A\u043B\u0430; \\r\\n\\t\\t\u0418\u043D\u0430\u0447\u0435\\r\\n\\t\\t\\t\u0412\u043E\u0437\u0432\u0440\u0430\u0442 \u0418\u0441\u0442\u0438\u043D\u0430; \\t\\t\\t\\r\\n\\t\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438; \\r\\n\\t\u041A\u043E\u043D\u0435\u0446\u0426\u0438\u043A\u043B\u0430;\\r\\n\\t\\r\\n\\t\u0412\u043E\u0437\u0432\u0440\u0430\u0442 \u041B\u043E\u0436\u044C; \\r\\n\u041A\u043E\u043D\u0435\u0446\u0424\u0443\u043D\u043A\u0446\u0438\u0438 // \u041E\u0431\u044A\u0435\u043A\u0442\u0418\u0437\u043C\u0435\u043D\u0435\u043D\\r\\n\\r\\n// \u0412\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u0435\u0442 \u041C\u0430\u0441\u0441\u0438\u0432 \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u044B\u0445 \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u043E\u0432 \u0441 \u0443\u0447\u0435\u0442\u043E\u043C \u043F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0445 \u0438 \u0438\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C\u044B\u0445 \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u043E\u0432\\r\\n// \\r\\n//\u041F\u0410\u0420\u0410\u041C\u0415\u0422\u0420\u042B\\r\\n//  \u0418\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B\: \u0421\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0435, \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E. \u0412\u0445\u043E\u0434\u043D\u043E\u0439.\\r\\n// \u0421\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0435, \u0432 \u043A\u043E\u0442\u043E\u0440\u043E\u043C \u043A\u043B\u044E\u0447\u043E\u043C \u044F\u0432\u043B\u044F\u0435\u0442\u0441\u044F \u0438\u043C\u044F \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u043E\u0433\u043E \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u0430/\u0442\u0430\u0431. \u0447\u0430\u0441\u0442\u0438, \u0430 \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u0435\u043C - \u0441\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430,\\r\\n// \u0445\u0430\u0440\u0430\u043A\u0442\u0435\u0440\u0438\u0437\u0443\u044E\u0449\u0430\u044F \u0441\u0442\u0430\u0440\u043E\u0435 \u0438 \u043D\u043E\u0432\u043E\u0435 \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u0435, \u0438\u043C\u044F \u0441\u0442\u0440\u043E\u043A\u0438 \u0438 \u043A\u043E\u043B\u043E\u043D\u043A\u0438 \u0422\u0427\\r\\n//  (\u0418\u043C\u044F \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u0430) -\\u003e \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430(\u0421\u0442\u0430\u0440\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435, \u041D\u043E\u0432\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435)\\r\\n//  (\u0418\u043C\u044F \u0422\u0427) -\\u003e (\u0418\u043C\u044F \u043A\u043E\u043B\u043E\u043D\u043A\u0438) -\\u003e \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430(\u0421\u0442\u0430\u0440\u043E\u0435\u041A\u043E\u043B\u0421\u0442\u0440\u043E\u043A, \u041D\u043E\u0432\u043E\u0435\u041A\u043E\u043B\u0421\u0442\u0440\u043E\u043A), \u0435\u0441\u043B\u0438 \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E \u0441\u0442\u0440\u043E\u043A \u0440\u0430\u0437\u043B\u0438\u0447\u0430\u044E\u0442\u0441\u044F\\r\\n//  (\u0418\u043C\u044F \u0422\u0427) -\\u003e (\u0418\u043C\u044F \u043A\u043E\u043B\u043E\u043D\u043A\u0438) -\\u003e \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430(\u0421\u0442\u0430\u0440\u043E\u0435\u041A\u043E\u043B\u0421\u0442\u0440\u043E\u043A, \u041D\u043E\u0432\u043E\u0435\u041A\u043E\u043B\u0421\u0442\u0440\u043E\u043A), \u0435\u0441\u043B\u0438 \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E \u0441\u0442\u0440\u043E\u043A \u0440\u0430\u0437\u043B\u0438\u0447\u0430\u044E\u0442\u0441\u044F\\r\\n//  (\u0418\u043C\u044F \u0422\u0427) -\\u003e (\u0418\u043C\u044F \u043A\u043E\u043B\u043E\u043D\u043A\u0438) -\\u003e \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430(\u0421\u0442\u0430\u0440\u043E\u0435\u041A\u043E\u043B\u0421\u0442\u0440\u043E\u043A, \u041D\u043E\u0432\u043E\u0435\u041A\u043E\u043B\u0421\u0442\u0440\u043E\u043A, \u041D\u043E\u043C\u0435\u0440\u0421\u0442\u0440\u043E\u043A\u0438, \u0418\u043C\u044F\u041A\u043E\u043B\u043E\u043D\u043A\u0438, \u0421\u0442\u0430\u0440\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435, \u041D\u043E\u0432\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435), \\r\\n//\\t\\t\u0435\u0441\u043B\u0438 \u043A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E \u0441\u0442\u0440\u043E\u043A \u0441\u043E\u0432\u043F\u0430\u0434\u0430\u044E\u0442, \u0430 \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u044F \u0432 \u043A\u043E\u043B\u043E\u043D\u043A\u0435 (\u0418\u043C\u044F \u043A\u043E\u043B\u043E\u043D\u043A\u0438) \u0440\u0430\u0437\u043B\u0438\u0447\u0430\u044E\u0442\u0441\u044F\\r\\n// \u0415\u0441\u043B\u0438 \u043F\u043E\u041E\u0431\u044A\u0435\u043A\u0442 - \u043D\u043E\u0432\u044B\u0439 (\u0435\u0449\u0435 \u043D\u0435 \u0437\u0430\u043F\u0438\u0441\u0430\u043D\u043D\u044B\u0439) \u043E\u0431\u044A\u0435\u043A\u0442, \u0432\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u0435\u0442 \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0435\\r\\n//\\t\\"\u0421\u0441\u044B\u043B\u043A\u0430\\" -\\u003e \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430(\u0421\u0442\u0430\u0440\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435, \u041D\u043E\u0432\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435, \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E, \u0421\u0441\u044B\u043B\u043A\u0430\u041D\u043E\u0432\u043E\u0433\u043E\u041E\u0431\u044A\u0435\u043A\u0442\u0430)\\r\\n//  \u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B\: \u0421\u0442\u0440\u043E\u043A\u0430, \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430, \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E (\u043D\u0435\u043E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u044B\u0439). \u0421\u043F\u0438\u0441\u043E\u043A \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u043E\u0432 \u043E\u0431\u044A\u0435\u043A\u0442\u0430 \u0447\u0435\u0440\u0435\u0437 \u0437\u0430\u043F\u044F\u0442\u0443\u044E, \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043F\u0440\u043E\u0432\u0435\u0440\u044F\u044E\u0442\u0441\u044F \u043D\u0430 \u043E\u0442\u043B\u0438\u0447\u0438\u0435.\\r\\n//\\t\\t\u0415\u0441\u043B\u0438 \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E \u0438\u043B\u0438 \u043D\u0435 \u0443\u043A\u0430\u0437\u0430\u043D, \u0442\u043E \u043F\u0440\u043E\u0432\u0435\u0440\u044F\u044E\u0442\u0441\u044F \u0432\u0441\u0435.\\r\\n//\\t\\t\u0415\u0441\u043B\u0438 \u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \\u003d \\"\u0421\u0441\u044B\u043B\u043A\u0430\\", \u0442\u043E \u0432 \u0441\u043B\u0443\u0447\u0430\u0435, \u0435\u0441\u043B\u0438 \u043E\u0431\u044A\u0435\u043A\u0442 \u0435\u0449\u0435 \u043D\u0435 \u0441\u043E\u0445\u0440\u0430\u043D\u044F\u043B\u0441\u044F \u0432\u0435\u0440\u043D\u0435\u0442 \u0418\u0441\u0442\u0438\u043D\u0430, \u0430 \u0432 \u0438\u043D\u044B\u0445 \u0441\u043B\u0443\u0447\u0430\u044F\u0445 - \u041B\u043E\u0436\u044C.   \\r\\n//\\t\\t\u0415\u0441\u043B\u0438 \u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \\u003d \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430, \u0442\u043E \u0434\u043B\u044F \u0442\u0430\u0431\u043B\u0438\u0447\u043D\u044B\u0445 \u0447\u0430\u0441\u0442\u0435\u0439 \u043C\u043E\u0436\u043D\u043E \u0443\u043A\u0430\u0437\u0430\u0442\u044C \u0432 \u043A\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u044F \u0432\u043B\u043E\u0436\u0435\u043D\u043D\u0443\u044E \u0441\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0443 \u0441 \u043A\u043B\u044E\u0447\u0430\u043C\u0438 - \u0438\u043C\u0435\u043D\u0430\u043C\u0438 \u043A\u043E\u043B\u043E\u043D\u043E\u043A \u0434\u043B\u044F \u0443\u0447\u0430\u0441\u0442\u0438\u044F \u0432 \u043F\u0440\u043E\u0432\u0435\u0440\u043A\u0435.   \\r\\n//  \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B\: \u0421\u0442\u0440\u043E\u043A\u0430, \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430, \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E (\u043D\u0435\u043E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u044B\u0439). \u0421\u043F\u0438\u0441\u043E\u043A \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u043E\u0432 \u043E\u0431\u044A\u0435\u043A\u0442\u0430 \u0447\u0435\u0440\u0435\u0437 \u0437\u0430\u043F\u044F\u0442\u0443\u044E, \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043D\u0435 \u043F\u0440\u043E\u0432\u0435\u0440\u044F\u044E\u0442\u0441\u044F \u043D\u0430 \u043E\u0442\u043B\u0438\u0447\u0438\u0435.\\r\\n//\\t\\t\u0415\u0441\u043B\u0438 \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E \u0438\u043B\u0438 \u043D\u0435 \u0443\u043A\u0430\u0437\u0430\u043D, \u0442\u043E \u043F\u0440\u043E\u0432\u0435\u0440\u044F\u044E\u0442\u0441\u044F \u0432\u0441\u0435.\\r\\n//\\t\\t\u0415\u0441\u043B\u0438 \u0432 \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \u0443\u043A\u0430\u0437\u0430\u043D \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442 \\"\u0421\u0441\u044B\u043B\u043A\u0430\\", \u0442\u043E \u0432 \u0441\u043B\u0443\u0447\u0430\u0435, \u0435\u0441\u043B\u0438 \u043E\u0431\u044A\u0435\u043A\u0442 \u0435\u0449\u0435 \u043D\u0435 \u0441\u043E\u0445\u0440\u0430\u043D\u044F\u043B\u0441\u044F \u0432\u0435\u0440\u043D\u0435\u0442 \u041B\u043E\u0436\u044C \\r\\n//\\t\\t\u0415\u0441\u043B\u0438 \u0432 \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \u0443\u043A\u0430\u0437\u0430\u043D\u0430 \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430, \u0442\u043E \u0434\u043B\u044F \u0442\u0430\u0431\u043B\u0438\u0447\u043D\u044B\u0445 \u0447\u0430\u0441\u0442\u0435\u0439 \u043C\u043E\u0436\u043D\u043E \u0443\u043A\u0430\u0437\u0430\u0442\u044C \u0432 \u043A\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0437\u043D\u0430\u0447\u0435\u043D\u0438\u044F \u0432\u043B\u043E\u0436\u0435\u043D\u043D\u0443\u044E \u0441\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0443 \u0441 \u043A\u043B\u044E\u0447\u0430\u043C\u0438 - \u0438\u043C\u0435\u043D\u0430\u043C\u0438 \u043A\u043E\u043B\u043E\u043D\u043E\u043A \u0434\u043B\u044F \u0438\u0441\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u044F \u0438\u0437 \u043F\u0440\u043E\u0432\u0435\u0440\u043A\u0438.   \\r\\n//\u0412\u041E\u0417\u0412\u0420\u0410\u0429\u0410\u0415\u0422\\r\\n//\\t\u041C\u0430\u0441\u0441\u0438\u0432. \u0421\u043F\u0438\u0441\u043E\u043A \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u044B\u0445 \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u043E\u0432 \u0441 \u0443\u0447\u0435\u0442\u043E\u043C \u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \u0438 \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B, \u043A\u0430\u0436\u0434\u044B\u0439 \u044D\u043B\u0435\u043C\u0435\u043D\u0442 \u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E \u043F\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043B\u044F\u0435\u0442\:\\r\\n//\\t\\t- \u0434\u043B\u044F \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u044B\u0445 \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u043E\u0432 \u0448\u0430\u043F\u043A\u0438 - \u0438\u043C\u044F \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u0430\\r\\n//\\t\\t- \u0434\u043B\u044F \u0438\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u043E\u0433\u043E \u0440\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u0430 \u0422\u0427 - \u0441\u0442\u0440\u043E\u043A\u0430 \u0432\u0438\u0434\u0430 [\u0438\u043C\u044F \u0442\u0430\u0431. \u0447\u0430\u0441\u0442\u0438].[\u0438\u043C\u044F \u043A\u043E\u043B\u043E\u043D\u043A\u0438 \u0442\u0430\u0431. \u0447\u0430\u0441\u0442\u0438]\\r\\n\u0424\u0443\u043D\u043A\u0446\u0438\u044F \u041F\u043E\u043B\u0443\u0447\u0438\u0442\u044C\u0421\u043F\u0438\u0441\u043E\u043A\u0418\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u044B\u0445\u0420\u0435\u043A\u0432\u0438\u0442\u043E\u0432(\u0418\u0437\u043C\u0435\u043D\u0435\u043D\u043D\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B, \u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \\u003d \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E, \u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \\u003d \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E) \u042D\u043A\u0441\u043F\u043E\u0440\u0442\\r\\n\\t\u0420\u0435\u0437\u0443\u043B\u044C\u0442\u0430\u0442 \\u003d \u041D\u043E\u0432\u044B\u0439 \u041C\u0430\u0441\u0441\u0438\u0432;\\r\\n\\t\\r\\n\\t\u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0442\u044C\u041F\u043E\u0421\u043F\u0438\u0441\u043A\u0443 \\u003d (\u041F\u0440\u043E\u0432\u0435\u0440\u044F\u0435\u043C\u044B\u0435\u0420\u0435\u043A\u0432\u0438\u0437\u0438\u0442\u044B \\u003c\\u003e \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E);\\r\\n\\t\u0418\u0441\u043A\u043B\u044E\u0447\u0430\u0442\u044C\u041F\u043E\u0421\u043F\u0438\u0441\u043A\u0443 \\u003d (\u04