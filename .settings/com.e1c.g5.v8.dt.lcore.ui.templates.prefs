\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438;\\r\\n\\t\\r\\n\\t\u0421\u0442\u0430\u0440\u044B\u0439\u041E\u0431\u044A\u0435\u043A\u0442\u041E\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \\u003d \u041B\u043E\u0436\u044C;\\r\\n\\t\u0415\u0441\u043B\u0438 \u043B\u0430\u0441\u0421\u0417 \\u003d \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\u043B\u0437\u0421\u0442\u0430\u0440\u044B\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u044F \\u003d \u041D\u043E\u0432\u044B\u0439 \u0417\u0430\u043F\u0440\u043E\u0441(\\r\\n\\t\\t\\t\\"\u0412\u042B\u0411\u0420\u0410\u0422\u042C *\\r\\n\\t\\t\\t|\u0418\u0417 \\"+\u043F\u043E\u041E\u0431\u044A\u0435\u043A\u0442.\u041C\u0435\u0442\u0430\u0434\u0430\u043D\u043D\u044B\u0435().\u041F\u043E\u043B\u043D\u043E\u0435\u0418\u043C\u044F()+\\" \u041A\u0410\u041A \u0422\u0430\u0431\\r\\n\\t\\t\\t|\u0413\u0414\u0415 \u0422\u0430\u0431.\u0421\u0441\u044B\u043B\u043A\u0430 \\u003d \\u0026\u0421\u0441\u044B\u043B\u043A\u0430\\");\\r\\n\\t\\t\u043B\u0437\u0421\u0442\u0430\u0440\u044B\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u044F.\u0423\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u044C\u041F\u0430\u0440\u0430\u043C\u0435\u0442\u0440(\\"\u0421\u0441\u044B\u043B\u043A\u0430\\", \u043F\u043E\u041E\u0431\u044A\u0435\u043A\u0442.\u0421\u0441\u044B\u043B\u043A\u0430);\\r\\n\\t\\t\u043B\u0430\u0442\u0421\u0442\u0430\u0440\u044B\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u044F \\u003d \u043B\u0437\u0421\u0442\u0430\u0440\u044B\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u044F.\u0412\u044B\u043F\u043E\u043B\u043D\u0438\u0442\u044C().\u0412\u044B\u0433\u0440\u0443\u0437\u0438\u0442\u044C();\\r\\n\\t\\t\u0415\u0441\u043B\u0438 \u043B\u0430\u0442\u0421\u0442\u0430\u0440\u044B\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u044F.\u041A\u043E\u043B\u0438\u0447\u0435\u0441\u0442\u0432\u043E() \\u003d 0 \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\\t\u0421\u0442\u0430\u0440\u044B\u0439\u041E\u0431\u044A\u0435\u043A\u0442\u041E\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \\u003d \u0418\u0441\u0442\u0438\u043D\u0430;\\r\\n\\t\\t\u0418\u043D\u0430\u0447\u0435\\r\\n\\t\\t\\t\u043B\u0430\u0441\u0421\u0417 \\u003d \u043B\u0430\u0442\u0421\u0442\u0430\u0440\u044B\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u044F[0];\\r\\n\\t\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438;\\r\\n\\t\u0418\u043D\u0430\u0447\u0435\u0415\u0441\u043B\u0438 \u043D\u0435 \u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435\u0417\u0430\u043F\u043E\u043B\u043D\u0435\u043D\u043E(\u043B\u0430\u0441\u0421\u0417) \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\u0421\u0442\u0430\u0440\u044B\u0439\u041E\u0431\u044A\u0435\u043A\u0442\u041E\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \\u003d \u0418\u0441\u0442\u0438\u043D\u0430;\\r\\n\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438; \\r\\n\\t\u0415\u0441\u043B\u0438 \u0421\u0442\u0430\u0440\u044B\u0439\u041E\u0431\u044A\u0435\u043A\u0442\u041E\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \\u003d \u0418\u0441\u0442\u0438\u043D\u0430 \u0422\u043E\u0433\u0434\u0430\\r\\n\\t\\t\u043B\u0430\u0445\u0420\u0435\u0437[\\"\u0421\u0441\u044B\u043B\u043A\u0430\\"] \\u003d \u041D\u043E\u0432\u044B\u0439 \u0421\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0430(\\"\u0421\u0442\u0430\u0440\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435, \u041D\u043E\u0432\u043E\u0435\u0417\u043D\u0430\u0447\u0435\u043D\u0438\u0435\\", \u041D\u0435\u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u043E, \u043F\u043E\u041E\u0431\u044A\u0435\u043A\u0442.\u041F\u043E\u043B\u0443\u0447\u0438\u0442\u044C\u0421\u0441\u044B\u043B\u043A\u0443\u041D\u043E\u0432\u043E\u0433\u043E());\\r\\n\\t\\t\u0412\u043E\u0437\u0432\u0440\u0430\u0442 \u043B\u0430\u0445\u0420\u0435\u0437;\\r\\n\\t\u041A\u043E\u043D\u0435\u0446\u0415\u0441\u043B\u0438; \\r\\n\\t\u0414\u043B\u044F \u043A\u0430